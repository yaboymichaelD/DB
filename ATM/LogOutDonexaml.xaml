        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ATM"
        mc:Ignorable="d"
        Title="Logged Out" Height="450" Width="800" Loaded="Window_Loaded">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="19*"/>
            <ColumnDefinition Width="113*"/>
        </Grid.ColumnDefinitions>
        <Label Content="You have been logged out. " Grid.Column="1" HorizontalAlignment="Left" Margin="231,171,0,0" VerticalAlignment="Top"/>
        <Label Content="Please take your card and your receipt." Grid.Column="1" HorizontalAlignment="Left" Margin="199,226,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
